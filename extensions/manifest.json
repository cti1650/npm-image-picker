{
  "name": "npm Image Picker Extension",
  "description": "npm Image Picker Extension",
  "version": "0.0.8",
  "icons": {
    "16": "/icons/icon-16x16.png",
    "48": "/icons/icon-48x48.png",
    "128": "/icons/icon-128x128.png",
    "19": "/icons/icon-19x19.png"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "host_permissions": [
    "https://www.npmjs.com/*"
  ],
  "permissions": [
    "tabs",
    "scripting"
  ],
  "manifest_version": 3
}